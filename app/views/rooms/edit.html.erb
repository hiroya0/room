<h3>部屋編集</h3>

<%= form_with model: @room do |f| %>
  <table>
    <tr>
    <th><%= f.label :room_image,"画像の登録"%></th>
    <td><%= f.file_field :room_image%></td>
    </tr>
    <tr>
      <th><%= f.label :name ,"名前" %></th>
      <td><%= f.text_field :name %></td>
    </tr>
    <tr>
      <th><%= f.label :introduction ,"部屋詳細" %></th>
      <td><%= f.text_field :introduction %></td>
    </tr>
    <tr>
      <th><%= f.label :address ,"住所" %></th>
      <td><%= f.text_field :address %></td>
    </tr>
    <tr>
      <th><%= f.label :price ,"価格" %></th>
      <td><%= f.text_area :price %></td>
    </tr>
  </table>

  <div>
    <ul>
      <li><%= f.submit "編集を完了する" %></li>
    </ul>
    <a href="javascript:history.back()">[戻る]</a>
  </div>
<% end %>